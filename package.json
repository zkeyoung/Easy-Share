    "@electron-forge/maker-zip": "^6.0.5",
    "@types/express": "^4.17.17",
    "electron": "^23.1.1",
    "typescript": "^4.9.5",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "electron-squirrel-startup": "^1.0.0"
